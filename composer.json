{
    "name": "charsen/laravel-scaffold",
    "description": "Scaffold for Laravel",
    "keywords": [
        "charsen",
        "laravel-scaffold"
    ],
    "homepage": "https://github.com/charsen/laravel-scaffold",
    "license": "MIT",
    "authors": [
        {
            "name": "Charsen",
            "email": "780537@qq.com",
            "homepage": "https://www.charsen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Charsen\\Scaffold\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Charsen\\Scaffold\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Charsen\\Scaffold\\ScaffoldProvider"
            ]
        }
    }
}
